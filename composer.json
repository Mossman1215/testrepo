{
    "name": "silverstripe/recipe-elemental",
    "type": "silverstripe-recipe",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": ">=0.1@dev <1.0",
        "silverstripe/recipe-cms": "1.0.x-dev",
        "silverstripe-themes/simple": "~3.2.0",
        "dnadesign/silverstripe-elemental": "dev-ss4 as dev-master",
        "dnadesign/silverstripe-elemental-skeletons": "dev-master",
        "silverstripe/elemental-blocks": "1.0.x-dev"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "project-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "mysite/.htaccess",
            "mysite/_config.php",
            "mysite/_config/mysite.yml",
            "mysite/code/Page.php",
            "mysite/code/PageController.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:creative-commoners/silverstripe-elemental.git"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev"
}
