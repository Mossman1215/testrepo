{
    "name": "cwp/cwp-recipe-kitchen-sink",
    "type": "silverstripe-recipe",
    "description": "Internal testing CWP recipe for all modules, optional and suggested",
    "homepage": "https://www.cwp.govt.nz",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-plugin": "^1",
        "cwp/cwp-installer": "2.1.x-dev",
        "cwp/agency-extensions": "2.0.x-dev",
        "cwp/watea-theme": "2.0.x-dev",
        "silverstripe/controllerpolicy": "2.0.x-dev",
        "silverstripe/crontask": "2.1.x-dev",
        "silverstripe/gridfieldqueuedexport": "2.1.x-dev",
        "silverstripe/ldap": "1.0.x-dev",
        "silverstripe/recipe-content-blocks": "2.0.x-dev",
        "dnadesign/silverstripe-elemental-subsites": "2.0.x-dev",
        "dnadesign/silverstripe-elemental-userforms": "2.0.x-dev",
        "bringyourownideas/silverstripe-composer-update-checker": "dev-pulls/2.0/composer-home as 2.0.x-dev"
    },
    "repositories": [
        {"type": "vcs", "url": "git@github.com:creative-commoners/silverstripe-composer-update-checker.git"}
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mikey179/vfsStream": "^1.6"
    },
    "extra": {
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/blog.yml",
            "app/_config/contentblocks.yml",
            "app/_config/cwpadmin.yml",
            "app/_config/environmentcheck.yml",
            "app/_config/mysite.yml",
            "app/_config/oembed.yml",
            "app/_config/theme.yml",
            "app/_config/versionfeed.yml",
            "app/src/BlocksPage.php",
            "app/src/HomePage.php",
            "app/src/HomePageController.php",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
